
type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  bio: String
  age: Int
  gender: GenderOptions
  contacts: [Contact!]!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

enum GenderOptions {
  MALE
  FEMALE
  OTHER
}

type Contact {
  id: ID! @id
  name: String!
  email: String
  phoneNumber: String
  jobTitle: String
  user: User
}
